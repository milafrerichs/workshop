<div class="list">

{.section collection}
  {.repeated section collections}
    <div class="list-item">
      {.section item}
        {.section customContent}
          <i class="fa {icon}"></i>
        {.end}
        <h1 data-content-field="title" class="entry-title p-name">{title}</h1>

        {.section body}
          {@}
        {.end}
      {.end}
    </div>
  {.end}
{.end}
</div>
